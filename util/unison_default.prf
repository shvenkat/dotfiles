# Sync settings.
times = true
auto = false
confirmbigdel = true
confirmmerge = true
contactquietly = true
logfile = /tmp/unison.log
sortbysize = true
sshargs = -C

# Backup settings.
backup = Name *
backuploc = central

# ----  EXCLUDED FILES  ----

## Ad-hoc backup files.
ignore = Name *~
ignore = Name .*~

## Python.
ignore = Name env
ignore = Name .venv
ignore = Name .mypy_cache
ignore = Name __pycache__
ignore = Name *.egg-info
ignore = Name *.pyc
ignore = Name .ipynb_checkpoints
ignore = Name .pytest_cache

## MacOS
ignore = Name .DS_Store

## Ephemera.
ignore = Name *.log
